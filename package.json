    "migration:run": "npm run typeorm migration:run -- -d ./src/shared/infra/typeorm/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/shared/infra/typeorm/data-source.ts",
    "seed:admin": "ts-node-dev ./src/shared/infra/typeorm/seed/admin.ts"
  },
